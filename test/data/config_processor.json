{
  "input": {
    "data_structure": "config_data_structure.json",
    "input_type": "kafka",
    "config": "config_processor.json",
    "options": {
      "server": "192.168.1.1",
      "port": 29092,
      "topic": "testtag01",
      "batchDuration": 10,
      "sep": ","
    }
  },
  "output": {
    "method": "stdout",
    "options": {
    }
  },
  "processing": {
    "transformation": [
      "packet_size",
      "traffic: mul(packet_size,sampling_rate)"
    ],
    "aggregations": {
      "operation_type": "reduce",
      "rule": [
        "min(packet_size)",
        "sum(traffic)"
      ]
    }
  },
  "databases": {
    "country": "./Mock-GeoLite2-Country.mmdb",
    "city": "./Mock-GeoLite2-City.mmdb",
    "asn": "./Mock-GeoLite2-ASN.mmdb"
  }
}
